{
	"name": "move-syntax-highlight",
	"displayName": "move-syntax-highlight",
	"description": "syntax hightlight for libra move language using tree-sitter",
	"license": "MIT",
	"preview": true,
	"author": {
		"name": "nonsense1984"
	},
	"repository": {
		"url": "https://github.com/move-hub/move-syntax-highlight"
	},
	"publisher": "movei",
	"version": "0.2.2",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Other"
	],
	"keywords": [
		"Syntax",
		"Highlight",
		"Semantic",
		"Highlightling",
		"Highlighter",
		"Coloring",
		"Tree-Sitter",
		"Parser",
		"Libra",
		"Move"
	],
	"activationEvents": [
		"onLanguage:move"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "move",
				"aliases": [
					"Move",
					"move"
				],
				"extensions": [
					".move"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "move",
				"scopeName": "source.move",
				"path": "./syntaxes/move.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"vscode:package": "npx vsce package",
		"vscode:publish": "npx vsce publish",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"postinstall": "node ./scripts/build_asset.js",
		"release": "standard-version"
	},
	"dependencies": {
		"web-tree-sitter": "^0.16.2"
	},
	"devDependencies": {
		"@movei/tree-sitter-move": "^0.2.4",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.0.22",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^3.7.0",
		"@typescript-eslint/parser": "^3.0.0",
		"eslint": "^7.5.0",
		"glob": "^7.1.6",
		"js-yaml": "^3.13.1",
		"mocha": "^7.0.1",
		"standard-version": "^9.0.0",
		"tree-sitter-cli": "^0.16.9",
		"typescript": "^3.9.7",
		"vsce": "^1.78.0",
		"vscode-test": "^1.3.0"
	}
}
